.obj/release-shared/yanglabel.o: yanglabel.cpp yanglabel.h \
		yang.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/yanglabel.o yanglabel.cpp

.obj/release-shared/main.o: main.cpp yanglabel.h \
		../qtopia-2.2.0-FriendlyARM/qtopia/include/qtopia/qpeapplication.h \
		yang.h \
		../qtopia-2.2.0-FriendlyARM/qtopia/src/libraries/qtopia/qpeapplication.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/main.o main.cpp

.ui/release-shared/yang.h: yang.ui 
	$(UIC) yang.ui -o .ui/release-shared/yang.h

.ui/release-shared/yang.cpp: .ui/release-shared/yang.h yang.ui 
	$(UIC) yang.ui -i yang.h -o .ui/release-shared/yang.cpp

.obj/release-shared/yang.o: .ui/release-shared/yang.cpp .ui/release-shared/yang.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/yang.o .ui/release-shared/yang.cpp

.obj/release-shared/moc_yang.o: .moc/release-shared/moc_yang.cpp  .ui/release-shared/yang.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_yang.o .moc/release-shared/moc_yang.cpp

.moc/release-shared/moc_yang.cpp: $(MOC) .ui/release-shared/yang.h
	$(MOC) .ui/release-shared/yang.h -o .moc/release-shared/moc_yang.cpp

####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/bin/" || $(MKDIR) "$(INSTALL_ROOT)/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/bin/"


install: install_target  

uninstall: uninstall_target  

